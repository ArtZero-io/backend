{
  "name": "api",
  "version": "0.0.1",
  "description": "New formating for Api and Jobs",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t api .",
    "docker:run": "docker run -p 3000:3000 -d api",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "private": true,
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^5.0.8",
    "@loopback/core": "^4.0.8",
    "@loopback/cron": "^0.9.8",
    "@loopback/repository": "^5.1.3",
    "@loopback/rest": "^12.0.8",
    "@loopback/rest-explorer": "^5.0.8",
    "@loopback/service-proxy": "^5.0.8",
    "@polkadot/api": "^10.2.2",
    "@polkadot/api-contract": "^10.2.2",
    "@polkadot/keyring": "^11.1.3",
    "@polkadot/util": "^11.1.3",
    "@polkadot/util-crypto": "^11.1.3",
    "@types/download": "^8.0.2",
    "@types/sharp": "^0.31.1",
    "axios": "^1.3.3",
    "csv": "^6.2.10",
    "dotenv": "^16.0.3",
    "download": "^8.0.0",
    "exceljs": "^4.3.0",
    "file-type": "^16.5.3",
    "ipfs": "^0.62.2",
    "ipfs-http-client": "^56.0.2",
    "is-ipfs": "^6.0.2",
    "it-to-stream": "^1.0.0",
    "loopback-connector-mongodb": "^5.2.3",
    "mobile-detect": "^1.4.5",
    "save-file-by-url": "^0.4.2",
    "sharp": "^0.30.4",
    "tslib": "^2.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.8",
    "@loopback/eslint-config": "^13.0.8",
    "@loopback/testlab": "^5.0.8",
    "@types/node": "^14.18.36",
    "eslint": "^8.33.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.9.4"
  }
}
